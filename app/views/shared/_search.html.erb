<div class="form-group form-group-lg has-feedback" id="search-form">
  <%= form_tag images_path, method: :get, class: "input-group", id: 'home-seach-form' do %>
      <%= text_field_tag :search, params[:search], class: "form-control" %>
      <span class="input-group-addon" id="search-form-submit">Search</span>
  <% end %>
  <div class="tags">
    <% @most_search_keys.each do |key| %>
      <%= link_to key, images_path(search: key) %>
    <% end %>
  </div>
</div>

<script>
  $("#search-form-submit").click(function(){
      $("form#home-seach-form").submit();
  })
</script>
